@model IEnumerable<CMSOnlineStore.Models.WievModels.Pages.PageVM>

@{
    string tempPageTitle = (string)TempData["PageTitle"];
}

@foreach (var item in Model)
{
    if (tempPageTitle == item.Title)
    {
        <li class="nav-item active"> <a class="nav-link" href="/@item.Slug">@item.Title</a> </li>
    }
    else
    {
        <li class="nav-item"> <a class="nav-link" href="/@item.Slug">@item.Title</a> </li>
    }
}


